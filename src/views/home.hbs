
<div class="home-container">

{{#each videos}}
	<div class="home-video">
		<div class="title"><b>{{_id}}</b> ({{count}})</div>
		<div class="subtitle"><a href="/list/{{_id}}">list</a>, <a href="/compilation/{{_id}}">compilation</a></div>
		<video class="video" id="v{{@index}}" preload="auto" width="100" height="100">
			<source src="https://skylapse.s3.amazonaws.com/videos/{{video.uid}}/{{video.id}}.mp4" type="video/mp4">
			<source src="https://skylapse.s3.amazonaws.com/videos/{{video.uid}}/{{video.id}}.ogg" type="video/ogg">
		</video>
	</div>
{{/each}}
</div>
