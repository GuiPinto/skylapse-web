<input type="checkbox" class="stacked-mode" value="1">Stacked Mode
<hr />

<div id="progress"></div>

<div class="wrap">
{{#each videos}}
	<video class="video" id="v{{@index}}" preload="none" controls>
		<source src="https://skylapse.s3.amazonaws.com/videos/{{uid}}/{{id}}.mp4" type="video/mp4">
		<source src="https://skylapse.s3.amazonaws.com/videos/{{uid}}/{{id}}.ogg" type="video/ogg">
	</video>
{{/each}}
</div>

<script type="text/javascript">
var videos = {{{videosJson}}};
</script>

<link href="http://refreshless.com/nouislider/source/distribute/jquery.nouislider.min.css" rel="stylesheet">
<link href="http://refreshless.com/nouislider/source/distribute/jquery.nouislider.pips.min.css" rel="stylesheet">
<script src="http://refreshless.com/nouislider/source/distribute/jquery.nouislider.all.js"></script>
<script src="/js/player.js" type="text/javascript"></script>
